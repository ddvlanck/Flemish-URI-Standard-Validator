{
  "name": "flemish-uri-standard-validator",
  "version": "1.0.5",
  "description": "npm package to check if a URI meets the requirements of the flemish URI standard for data",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc | webpack"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib/*.d.ts",
    "lib/*.js",
    "bin/*.d.ts",
    "bin/*.js",
    "dist"
  ],
  "author": "Dwight Van Lancker",
  "license": "ISC",
  "dependencies": {
    "follow-redirects": "^1.9.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.0",
    "request": "^2.88.0",
    "typescript": "^3.6.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.11",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
